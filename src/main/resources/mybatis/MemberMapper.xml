<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.bit.juti.mapper.MemberMapper"> <!-- 인터페이스에 대한 패키지.자바명 -->
	
	<insert id="addUser" parameterType="UserVo">
		insert into users (user_id,user_password,user_passwordCheck,user_name,user_birth,user_phonenumber,user_email,user_address) 
		values (#{user_id},#{user_password},#{user_passwordCheck},#{user_name},#{user_birth},#{user_phonenumber},#{user_email},#{user_address})
	</insert>
	
	

	
</mapper>
